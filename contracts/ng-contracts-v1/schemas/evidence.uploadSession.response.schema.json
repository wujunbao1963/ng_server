{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://neighborguard.dev/contracts/v1/evidence.uploadSession.response.schema.json",
  "title": "POST /api/circles/:circleId/events/:eventId/evidence/upload-session - Response",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "sessionId": {
      "$ref": "https://neighborguard.dev/contracts/v1/common.schema.json#/definitions/Uuid"
    },
    "uploadUrls": {
      "type": "array",
      "minItems": 1,
      "items": {
        "type": "object",
        "additionalProperties": false,
        "properties": {
          "sha256": {
            "$ref": "https://neighborguard.dev/contracts/v1/common.schema.json#/definitions/Sha256"
          },
          "url": {
            "type": "string",
            "format": "uri"
          }
        },
        "required": [
          "sha256",
          "url"
        ]
      }
    }
  },
  "required": [
    "sessionId",
    "uploadUrls"
  ]
}